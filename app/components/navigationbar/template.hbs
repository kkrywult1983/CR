<nav class='navbar navbar-expand-lg bg-warning'>
  <div class='container-fluid'>
    <button
      class='navbar-toggler'
      type='button'
      data-bs-toggle='collapse'
      data-bs-target='#navbarNav'
      aria-controls='navbarNav'
      aria-expanded='false'
      aria-label='Toggle navigation'
    >
      <span class='navbar-toggler-icon'></span>
    </button>
    <div class='collapse navbar-collapse' id='navbarNav'>
      <ul class='navbar-nav'>
        <li class='nav-item'>
          <LinkTo
            class='nav-link active'
            aria-current='page'
            @route='main'
          >CarRental</LinkTo>
        </li>
        {{! <li class='nav-item'>
          <LinkTo class='nav-link' @route='fleet'>Flota</LinkTo>
        </li> }}
        {{#if this.isUserLoggedIn}}
          <li class='nav-item'>
            {{! template-lint-disable link-href-attributes }}
            <LinkTo class='nav-link' @route='users'>Users list</LinkTo>
          </li>

          <li class='nav-item'>
            {{! template-lint-disable link-href-attributes }}
            <LinkTo class='nav-link' @route='settings'>Settings</LinkTo>
          </li>
          <li class='nav-item'>
            {{! template-lint-disable link-href-attributes }}
            <LinkTo class='nav-link' @route='chart'>Charts</LinkTo>
          </li>
        {{/if}}

        {{#unless this.isUserLoggedIn}}
          <li class='nav-item'>
            {{! template-lint-disable link-href-attributes }}
            <LinkTo class='nav-link' @route='login'>Login</LinkTo>
          </li>
          <li class='nav-item'>
            {{! template-lint-disable link-href-attributes }}
            <LinkTo class='nav-link' @route='register'>Register</LinkTo>
          </li>
        {{/unless}}
        {{#if this.isUserLoggedIn}}
          <li class='nav-item mx-50'>
            <span class='nav-link' role='button' {{on 'click' this.onLogout}}>
              LogOut
            </span>
          </li>
          <li class='nav-item me-50'>
            {{! template-lint-disable no-invalid-role }}
            <span class='nav-link' role='text'>
              Witaj:{{this.userLoggedInName}}

            </span>
          </li>
        {{/if}}
      </ul>
    </div>
  </div>
</nav>